                    "sFirst": "Đầu",
                    "sPrevious": "Trước",
                    "sNext": "Tiếp",
                    "sLast": "Cuối"
                }
            },
            "order": false,
            "deferRender": true,
            "autoWidth": true,
            "responsive": true,
            "columnDefs": [
                { "targets": 'no-sort', "orderable": false }
            ],
        });
