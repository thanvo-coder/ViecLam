            if (feather) {
                feather.replace({
                    width: 14,
                    height: 14
                });
            }
        });
        $(".dataTable").DataTable({
            "language": {
                "sProcessing": "Đang tải...",
                "sLengthMenu": "Xem _MENU_ bản ghi",
